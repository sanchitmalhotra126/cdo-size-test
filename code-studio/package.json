{
  "name": "@cdo/code-studio",
  "version": "0.0.1",
  "description": "Pipeline for static assets used by our Code Studio rails app (i.e. dashboard)",
  "author": "Code.org",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=0.10.25 <0.13",
    "npm": "^2.9.1"
  },
  "scripts": {
    "postinstall": "linklocal",
    "clean": "rm -rf build",
    "lint": "npm run lint:js -s",
    "lint:js": "jshint src/js/*.js",
    "build": "npm run build-js && npm run build-css",
    "build:min": "npm run build-js:min && npm run build-css:min",
    "build-css": "node-sass --include-path node_modules src/css/blockly_editor.css.scss build/css/blockly_editor.css",
    "prebuild-css": "mkdir -p build/css",
    "build-css:min": "node-sass --output-style compressed --include-path node_modules src/css/blockly_editor.css.scss build/css/blockly_editor.min.css",
    "prebuild-css:min": "mkdir -p build/css",
    "build-js": "browserify --debug src/js/blockly_editor.js -o build/js/blockly_editor.js",
    "prebuild-js": "mkdir -p build/js",
    "build-js:min": "browserify src/js/blockly_editor.js | uglifyjs -o build/js/blockly_editor.min.js",
    "prebuild-js:min": "mkdir -p build/js",
    "start": "echo \"Unimplemented.\"",
    "stop": "echo \"Unimplemented.\"",
    "test": "echo \"Unimplemented.\""
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "jshint": "^2.8.0",
    "node-sass": "^3.4.2",
    "uglify-js": "^2.6.1"
  },
  "dependencies": {
    "@cdo/code-studio-vendor": "file:./vendor",
    "codemirror": "5.5",
    "linklocal": "^2.5.2",
    "marked": "0.3.2"
  }
}
