{
  "name": "@cdo/code-studio",
  "version": "0.0.1",
  "description": "Pipeline for static assets used by our Code Studio rails app (i.e. dashboard)",
  "author": "Code.org",
  "license": "Apache-2.0",
  "config": {
    "engine-strict": true
  },
  "engines": {
    "node": ">=0.12.0",
    "npm": "^2.9.1"
  },
  "scripts": {
    "postinstall": "linklocal",
    "clean": "rm -rf build",
    "lint": "npm run lint-js -s && npm run lint-css -s",
    "lint-css": "sass-lint src/css/*css",
    "lint-js": "jshint src/js/*.js",
    "build": "npm run copy-assets && npm run build-js && npm run build-css",
    "build:dist": "npm run lint && npm test && npm run clean && npm run copy-assets && npm run build-css && npm run build-js:min && npm run uglify-js",
    "build:min": "npm run copy-assets && npm run build-js:min && npm run build-css:min",
    "build-css": "scripts/build-css.js",
    "build-css:min": "scripts/build-css.js --min",
    "build-js": "scripts/build-js.js",
    "build-js:min": "scripts/build-js.js --min",
    "copy-assets": "scripts/copy-assets.js",
    "start": "scripts/build-js.js --watch",
    "test": "echo \"Tests Unimplemented.\"",
    "uglify-js": "scripts/uglify-js.js"
  },
  "devDependencies": {
    "babelify": "^6.3.0",
    "browserify": "^12.0.1",
    "chalk": "^1.1.1",
    "commander": "^2.9.0",
    "envify": "^3.4.0",
    "factor-bundle": "^2.5.0",
    "jshint": "^2.8.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.4.2",
    "recursive-readdir-sync": "^1.0.6",
    "sass-lint": "^1.4.0",
    "semver": "^5.1.0",
    "uglify-js": "^2.6.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "@cdo/code-studio-vendor": "file:./vendor",
    "codemirror": "5.5",
    "jquery": "^1.11.3",
    "linklocal": "^2.5.2",
    "lodash": "^3.10.1",
    "marked": "0.3.2",
    "react": "^0.13.1",
    "video.js": "4.5.2"
  }
}
