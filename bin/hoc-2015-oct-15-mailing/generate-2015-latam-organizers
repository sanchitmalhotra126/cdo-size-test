#!/usr/bin/env ruby
require_relative 'mailing-list-utils'

LATAM_ORGANIZERS_2015 = query_subscribed_contacts(q: 'kind_s:"HocSignup2015" && hoc_country_s:("la","ar","br")') do |raw|
  raw['international'] = 'true'
  raw['organizer'] = 'true'
  raw
end
puts "#{LATAM_ORGANIZERS_2015.count} 2015 latam organizers loaded."
