<svg version="1.1" width="298" height="298" xmlns="http://www.w3.org/2000/svg">

  <filter id="backgroundBlur">
    <feGaussianBlur in="SourceGraphic" stdDeviation="5" />
    <feComponentTransfer>
      <feFuncA slope="0.5" type="linear"></feFuncA>
    </feComponentTransfer>
  </filter>

  <g id="centered-group" transform="translate(150,150)">
    <g id="background-group" filter="url(#backgroundBlur)"></g>
    <g id="foreground-group"></g>
  </g>
</svg>