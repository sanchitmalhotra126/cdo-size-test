= view 'mobilecheck.js'

%script{src: CDO.studio_url('/assets/js/tutorialExplorer.js')}

- tutorials = Tutorials.new(:tutorials)
- filters = []
- filters << {name: "grade", entries: [{name: "pre"}, {name: "k-5"}, {name: "6-8"}, {name: "9+"}]}
- filters << {name: "platform", entries: [{name: "web"}, {name: "ipad"}, {name: "android"}, {name: "iphone"}]}

= tutorials.to_json

#tutorials

:javascript

  $(document).ready(function() {
    // Send through some values that the JavaScript will need.
    window.tutorialExplorerManager = new TutorialExplorerManager({
      tutorials: #{tutorials.to_json},
      filters: #{filters.to_json}
      }
    );
  });
