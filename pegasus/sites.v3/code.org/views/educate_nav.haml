#about_nav
  %span.about_list
    -current_url = request.path_info
    -{ 'Educator resources'=>'/educate','Hour of Code'=>'/educate/hoc','New K-5 program'=>'/educate/k5','District partnerships'=>'/educate/districts','K-8 intro course'=>'/educate/20hr','Curriculum'=>'/educate/curriculum','Inspire students'=>'/educate/inspire', 'Educational videos'=>'/educate/videos', '3rd party'=>'/educate/3rdparty'}.each_pair do |label,url|
      - if current_url == url
        %span.about_list_item_selected= label
      -else
        %a.about_list_item{:href=>url}= label
      %hr.narrow_hr
