<%
require 'cdo/aws/cloudfront'

-%>
---
AWSTemplateFormatVersion: 2010-09-09
Description: Clone an existing RDS Aurora MySQL cluster.
Parameters:
  SourceClusterId:
    Type: String
    Default: 'production-aurora-cluster'
  CloneClusterId:
    Type: String
    Default: 'production-aurora-cluster-clone'
  DatabaseUsername:
    Type: String
    Default: admin
    MaxLength: 16
Resources:
<%=  component 'database', clone_existing_cluster: false %>


