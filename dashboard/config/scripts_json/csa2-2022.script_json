    {
      "key": "29787f22-318e-4caf-a8e1-505ce10f570d",
      "position": 1,
      "properties": {
        "description": "We have learned a lot about working with variables in a program today! Let's help this programmer fix an error in their code.",
        "name": "Swapping Values",
        "remarks": true
      },
      "seeding_key": {
        "activity_section.key": "29787f22-318e-4caf-a8e1-505ce10f570d",
