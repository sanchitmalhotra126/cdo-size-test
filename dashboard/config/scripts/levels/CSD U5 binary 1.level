    "start_blocks": "createCanvas(\"background\");\r\nsetPosition(\"background\", 45, 80, 230, 60);\r\nsetFillColor(\"gray\");\r\nrect(0, 0, 320, 400);\r\nvar bits = [];\r\n\r\ntextLabel(\"instructions\", \"Change the value of the left-most bit by typing it in the box below.\");\r\nsetPosition(\"instructions\", 30, 180, 240, 100);\r\nsetProperty(\"instructions\", \"font-size\", 20);\r\nsetProperty(\"instructions\", \"text-align\", \"center\");\r\ntextInput(\"newValue\", \"\");\r\nsetPosition(\"newValue\", 100, 270, 120, 50);\r\nsetProperty(\"newValue\", \"font-size\", 30);\r\nsetPosition(\"editButton\", 100, 340);\r\nsetPosition(\"saveButton\", 100, 340);\r\nonEvent(\"saveButton\", \"click\", updatePlaceValue);\r\ntextLabel(\"warning\", \"\");\r\nsetPosition(\"warning\", 25, 400, 260, 100);\r\nsetProperty(\"warning\", \"font-size\", 20);\r\nsetProperty(\"warning\", \"text-color\", \"red\");\r\nsetProperty(\"warning\", \"text-align\", \"center\");\r\n\r\n\r\nfor (var i = 0; i<5 ; i++) {\r\n  bits[i] = [];\r\n  bits[i].on = 0;\r\n  bits[i].value = Math.pow(2, i);\r\n  textLabel(\"bit\" + i, bits[i].value);\r\n  \r\n  setProperty(\"bit\" + i, \"font-size\", 30);\r\n  setProperty(\"bit\" + i, \"text-color\", \"#303030\");\r\n  setProperty(\"bit\" + i, \"text-align\", \"center\");\r\n  setPosition(\"bit\" + i, 230 - 45 * i, 90, 40, 20);\r\n  createCanvas(\"canvas\" + i);\r\n  setPosition(\"canvas\" + i, 230 - 45 * i, 85, 40,50);\r\n  setActiveCanvas(\"canvas\" + i);\r\n  setFillColor(\"black\");\r\n  rect(0,0, 350, 400);\r\n  onEvent(\"bit\" + i, \"click\", function(event) {\r\n    toggleBit(event.targetId.substring(3));\r\n  })\r\n  onEvent(\"canvas\" + i, \"click\", function(event) {\r\n    toggleBit(event.targetId.substring(6));\r\n  })\r\n}\r\n\r\n  bits[4].value = 0;\r\n  setText(\"bit4\", \"?\");\r\n\r\n  textLabel(\"number\", \"0\");\r\n  setProperty(\"number\", \"text-color\", \"#303030\");\r\n  setProperty(\"number\", \"font-size\", 30);\r\n  setProperty(\"number\", \"text-align\", \"center\");\r\n  setPosition(\"number\", 100, 20, 120, 20);\r\n\r\nfunction toggleBit(value) {\r\n  //toggle 1 or 0 in array\r\n  bits[value].on = 1 - bits[value].on;\r\n\r\n  //show or hide bit\r\n  \r\n  setActiveCanvas(\"canvas\" + value);\r\n  if (bits[value].on == 1) {\r\n    setFillColor(\"white\");\r\n  } else {\r\n    setFillColor(\"black\");\r\n  }\r\n  rect(0,0, 350,400);\r\n  \r\n  //recalculate number\r\n  recalculate();\r\n  \r\n  \r\n}\r\n\r\nfunction updatePlaceValue() {\r\n  var newNumber = parseInt(getText(\"newValue\"));\r\n  //check that it's a number\r\n  if (isNaN(newNumber)) {\r\n    //update display & number\r\n    bits[4].value = 0;\r\n    setText(\"bit4\", \"?\");\r\n     setText(\"warning\", \"Value must be a number.\");\r\n    \r\n    //recalculate number \r\n    recalculate();\r\n  }else {\r\n    //update display & number\r\n    bits[4].value = newNumber;\r\n    setText(\"bit4\", newNumber);\r\n    \r\n    setText(\"warning\", \"\");\r\n    \r\n    //recalculate number \r\n    recalculate();\r\n  }\r\n}\r\n\r\nfunction recalculate() {\r\n  var value = 0;\r\n  for (var i=0; i<5; i++) {\r\n    value += bits[i].value * bits[i].on;\r\n  }\r\n  setText(\"number\", value);\r\n}\r\n",
