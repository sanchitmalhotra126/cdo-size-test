loons[7] = new Balloons(\"balloon8.png\");\n    balloons[8] = new Balloons(\"balloon9.png\");\n    balloons[9] = new Balloons(\"balloon10.png\");\n\n    // display balloon images\n    \n  }\n\n  public static void createPopped() {\n    popped[0] = new Balloons(\"poppedTeal.png\");\n    popped[1] = new Balloons(\"poppedPurple.png\");\n    popped[2] = new Balloons(\"poppedOrange.png\");\n    popped[3] = new Balloons(\"poppedGreen.png\");\n    popped[4] = new Balloons(\"poppedBlue.png\");\n    popped[5] = new Balloons(\"poppedTeal.png\");\n    popped[6] = new Balloons(\"poppedPurple.png\");\n    popped[7] = new Balloons(\"poppedOrange.png\");\n    popped[8] = new Balloons(\"poppedGreen.png\");\n    popped[9] = new Balloons(\"poppedBlue.png\");\n  }\n\n  public static void createRollButton(String filename) {\n    this.rollButton = new RollButton(filename);\n  }\n}",
