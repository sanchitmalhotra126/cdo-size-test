      "Scene.java": {
        "text": "import org.code.theater.*;\nimport org.code.media.*;\n\npublic class Scene {\n\n  private String sceneTitle;\n  private String background;\n  private Stage myStage = Theater.stage;\n\n  public Scene(String sceneTitle, String background) {\n    this.sceneTitle = sceneTitle;\n    this.background = background;\n  }\n\n  public void setBackground(String imageFile) {\n    myStage.drawImage(imageFile, 0, 0, myStage.getWidth(), myStage.getHeight(), 0);\n  }\n\n  public void setTitle(String text, int xPos, int yPos, int rotation) {\n    myStage.drawText(text, xPos, yPos, Color.WHITE, Font.SANS, 20, rotation);\n  }\n\n  public void playScene() {\n    setBackground(background);\n    myStage.pause(0.5);\n    setText(sceneTitle, 20, 20, 0);\n  }\n}",
        "isVisible": true
      },
