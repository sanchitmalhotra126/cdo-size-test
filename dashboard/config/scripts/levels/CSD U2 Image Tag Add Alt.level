    "start_sources": "{\r\n  \"files\": [\r\n    {\r\n      \"name\": \"index.html\",\r\n      \"data\": \"<!DOCTYPE html>\\r\\n<html>\\r\\n  <head>\\r\\n  <\\/head>\\r\\n  <body>\\r\\n    <h1>My Travels<\\/h1>\\r\\n    <h2>Kyoto, Japan<\\/h2>\\r\\n      <img src=\\\"Lanterns.jpg\\\">\\r\\n    <p>Photo by Nicholas Dench from FreeImages<\\/p>\\r\\n    <br>\\r\\n    <h2>Paris, France<\\/h2>\\r\\n      <img src=\\\"Eiffel.jpg\\\">\\r\\n    <p>Photo by Benjamin Thorn from FreeImages<\\/p>\\r\\n    <br>\\r\\n    <h2>Rio de Janeiro, Brazil<\\/h2>\\r\\n      <img src=\\\"Brazil.jpg\\\">\\r\\n    <p>Photo by Joe Carey from FreeImages<\\/p>\\r\\n  <\\/body>\\r\\n<\\/html>\"\r\n    },\r\n{\r\n      \"name\": \"Brazil.jpg\",\r\n      \"url\": \"https://images.code.org/0fed4362c4e75c3f5c287eafef099553-image-1564080461224.jpg\"\r\n    },\r\n{\r\n      \"name\": \"Lanterns.jpg\",\r\n      \"url\": \"https://images.code.org/9ce21680e1a5efe685ccc1ac5054637f-image-1564080485351.jpg\"\r\n    },\r\n{\r\n      \"name\": \"Eiffel.jpg\",\r\n      \"url\": \"https://images.code.org/ec9fede2332cc5ce29acfced00c9527c-image-1564080660348.jpg\"\r\n    }\r\n  ]\r\n}",
