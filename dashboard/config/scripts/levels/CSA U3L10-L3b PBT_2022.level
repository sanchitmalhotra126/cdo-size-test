      "BasketballStats.java": {
        "text": "import java.util.Scanner;\nimport java.io.File;\nimport java.io.FileNotFoundException;\n\n/*\n * Analyzes data about basketball teams\n */\npublic class BasketballStats {\n\n  private String[] teamNames;\n  private int[] championshipWins;\n\n  /*\n   * Constructor to create an BasketballStats object\n   * with the specified number of values to read\n   */\n  public BasketballStats(int numValues) {\n    this.teamNames = new String[numValues];\n    this.championshipWins = new int[numValues];\n  }\n\n  /*\n   * Reads the data from the file into the teamNames\n   * or the championshipWins array based on choice\n   */\n  public void readData(String filename, int choice) {\n    File myFile = new File(filename);\n    Scanner fileReader = createScanner(myFile);\n\n    if (choice == 1) {\n      for (int index = 0; index < teamNames.length; index++) {\n        if (fileReader.hasNextLine()) {\n          teamNames[index] = fileReader.nextLine();\n        }\n      }\n    }\n    else {\n      for (int index = 0; index < championshipWins.length; index++) {\n        if (fileReader.hasNextInt()) {\n          championshipWins[index] = fileReader.nextInt();\n        }\n      }\n    }\n\n    fileReader.close();\n  }\n\n  /*\n   * Returns a Scanner object to read theFile\n   * or notifies the user if the file is not found\n   */\n  public Scanner createScanner(File theFile) {\n    Scanner tempScanner = null;\n\n    try {\n      tempScanner = new Scanner(theFile);\n    } catch(FileNotFoundException error) {\n      System.out.println(\"File not found.\");\n    }\n\n    return tempScanner;\n  }\n\n  /*\n   * Returns true if there are duplicate years in\n   * the championshipWins array, otherwise returns false\n   */\n  public boolean hasDuplicates() {\n    // TO DO #1: Traverse the championshipWins array to determine if there\n    // are duplicate values and return the result.\n\n\n    \n  }\n  \n}",
