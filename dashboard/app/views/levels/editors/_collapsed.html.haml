- xml_id = "#{xml_id}"
- collapse_xml = "#{xml_id}_collapse_xml"

.min_max_label{data: {toggle: "collapse", target: "##{collapse_xml}"}}
  Open/Close
.row.collapse.in{id: collapse_xml}
  .span8
    ~ f.text_area :start_blocks, placeholder: "#{xml_id}" + ' Blocks', rows: 4, value: @level.pretty_block("#{xml_id}")
  .span4{id: xml_id + '-preview'}
