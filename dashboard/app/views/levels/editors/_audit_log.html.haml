%table.audit_log
  %tr
    %th Date
    %th What Was Changed
    %th Who Changed It
  - if(@level.audit_log != nil)
    - aud_log = JSON.parse(@level.audit_log)
    - aud_log.last(10).each do |log|
      %tr
        %td= log["changed_at"]
        %td= log["changed"]
        %td= log["changed_by_email"]
