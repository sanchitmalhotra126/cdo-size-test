-@task = @plc_enrollment_task_assignment.plc_task

%p#notice= notice

%p
  This is the task assignment completion page for #{@task.name}

-unless @task.type == :plc_task
  %p
    Here is the subpage for actually completing the task with type #{@task.type.titleized_task_name}
  = render partial: 'plc/enrollment_task_assignments/complete/' + @task.type.underscore_task_name

%p
  = link_to @task.resource_url

= link_to 'Edit', edit_plc_enrollment_task_assignment_path(@plc_enrollment_task_assignment)
\|
= link_to 'Back', plc_enrollment_task_assignments_path
